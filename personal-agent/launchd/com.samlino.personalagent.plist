<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.samlino.personalagent</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/samlino/lavprishjemmeside.dk/personal-agent/.venv/bin/python</string>
        <string>-m</string>
        <string>agent.main</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/samlino/lavprishjemmeside.dk/personal-agent</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>HOME</key>
        <string>/Users/samlino</string>
        <key>PATH</key>
        <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin</string>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/samlino/lavprishjemmeside.dk/personal-agent/audit/logs/system/stdout.log</string>

